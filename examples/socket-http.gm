SERVER_IP = "62.255.249.140";
SERVER_PORT = 80;

log("Connecting to "+SERVER_IP+":"+SERVER_PORT);

s = Socket();
if(!s.open(SERVER_IP, SERVER_PORT)) { fatal("FAILED!"); }

log("Sending request to server.");
if(!s.send(
	"GET /service/registerDevice?deviceId=1 HTTP/1.1\r\n"
	"Host: "+SERVER_IP+"\r\n"
	"Connection: keep-alive\r\n"
	"\r\n\r\n")) { fatal("SENDING FAILED!"); }

log("Waiting for response");
sleep(1.0);
while(s.receive(1024) > 0)
{
	print(""+s.buffer);
}
